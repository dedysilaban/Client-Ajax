@{
    ViewData["Title"] = "Login";
    Layout = "_LayoutL";
}

<div class="container">
    <div class="text-center">
        <div class="error mx-auto text-white" data-text="401">401</div>
    </div>
</div>

@section scripts{
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script>
        Swal.fire({
            icon: 'error',
            title: 'Error!!!',
            text: 'UNAUTHORIZE 401 !!!!'
        }).then(function () {
            window.location = "/Logins";
        });
    </script>
}