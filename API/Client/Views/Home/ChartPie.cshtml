@{ ViewData["Title"] = "Home Page"; Layout = "_LayoutTemp"; }

<div class="container-fluid">
    <h2 class="h3 mb-2 text-gray-800">List Chart</h2>
    <p>
       
    </p>
    <div class="row">
        <div class="col-xl-8 col-lg-7">
            <div class="card shadow mb-4">
                <div class="card-header py-3">
                    <h6 class="m-0 font-weight-bold text-primary">Chart University</h6>
                </div>
                <div class="card-body">
                    <div class="chart-pie" id="UChart">
                        <canvas></canvas>
                    </div>
                </div>
            </div>
        </div>
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">Chart Gender</h6>
            </div>
            <div class="card-body">
                <div class="chart-pie" id="GChart">
                    <canvas></canvas>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xl-8 col-lg-7">
        <div class="card shadow mb-4">
            <div class="card-header py-2">
                <h6 class="m-0 font-weight-bold text-primary">Chart Dummy</h6>
            </div>
            <div class="card-body">
                <div class="chart-pie" id="Chart">
                    <canvas></canvas>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts {
    <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
    <script src="~/Temp/js/ChartPie.js" asp-append-version="true"></script>
}
